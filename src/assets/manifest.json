{
    "id": "org.elastos.trinity.dapp.dappstore",
    "version": "1.1.1",
    "version_code": 10,
    "name": "Capsule Marketplace",
    "short_name": "Capsule Marketplace",
    "short_description": "Marketplace for the latest elastOS Capsules",
    "description": "Your one stop shop for the latest decentralized capsules. Simply click the capsule you wish to browse",
    "start_url": "index.html",
    "type": "app",
    "start_visible": "hide",
    "category": "tools",
    "icons": [
        {
            "src": "assets/images/logo.png",
            "sizes": "512x512",
            "type": "image/png"
        },
        {
            "src": "assets/images/logo.png",
            "sizes": "128x128",
            "type": "image/png"
        }
    ],
    "author": {
        "name": "Elastos Foundation",
        "email": "contact@elastos.org",
        "website": "https://www.elastos.org"
    },
    "default_locale": "en",
    "plugins": [
        "Device",
        "NetworkStatus",
        "File"
    ],
    "urls": [
        "https://dapp-store.elastos.org/apps/*"
    ],
    "background_color": "#4e8ef7",
    "theme_display": "show",
    "theme_color": "#4e8ef7",
    "intent_filters": [
        {
            "action": "appdetails"
        }
    ]
}
