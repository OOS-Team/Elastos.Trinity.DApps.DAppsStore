{
    "id": "org.elastos.trinity.dapp.dappstore",
    "version": "1.0.7",
    "version_code": 8,
    "name": "DApp Store",
    "short_name": "DApp Store",
    "short_description": "elast0S DApp Store",
    "description": "elast0S official app store, your one stop shop for the latest decentralized applications. Simply find the app you wish to add and click install",
    "start_url": "index.html",
    "type": "app",
    "start_visible": "hide",
    "category": "tools",
    "icons": [
        {
            "src": "assets/images/logo.png",
            "sizes": "512x512",
            "type": "image/png"
        },
        {
            "src": "assets/images/logo.png",
            "sizes": "128x128",
            "type": "image/png"
        }
    ],
    "author": {
        "name": "Elastos Foundation",
        "email": "contact@elastos.org",
        "website": "https://www.elastos.org"
    },
    "default_locale": "en",
    "plugins": [
        "Device",
        "NetworkStatus",
        "File"
    ],
    "urls": [
        "http://www.baidu.com/*",
        "https://www.google.com",
        "http://192.168.31.103:21636/*",
        "https://dapp-store.elastos.org/apps/*"
    ],
    "background_color": "#4e8ef7",
    "theme_display": "show",
    "theme_color": "#4e8ef7",
    "intent_filters": [
        {
            "action": "appdetails"
        }
    ]
}
